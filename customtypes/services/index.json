{
  "format": "page",
  "id": "services",
  "label": "Services",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "heading": {
        "type": "Text",
        "config": {
          "label": "Heading",
          "placeholder": ""
        }
      },
      "lead": {
        "type": "Text",
        "config": {
          "label": "Lead",
          "placeholder": ""
        }
      },
      "wave": {
        "type": "Select",
        "config": {
          "label": "Wave",
          "placeholder": "",
          "options": ["default", "1", "2", "3", "4", "5", "6", "7", "8"]
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "vertical_steps_with_images": {
              "type": "SharedSlice"
            },
            "faqs": {
              "type": "SharedSlice"
            },
            "section": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    },
    "Pricing Table": {
      "price_heading": {
        "type": "Text",
        "config": {
          "label": "Heading",
          "placeholder": ""
        }
      },
      "price_lead": {
        "type": "Text",
        "config": {
          "label": "Lead",
          "placeholder": ""
        }
      },
      "price": {
        "type": "Number",
        "config": {
          "label": "Price",
          "placeholder": ""
        }
      },
      "price_features": {
        "type": "StructuredText",
        "config": {
          "label": "Features",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,list-item,o-list-item"
        }
      },
      "has_booking": {
        "type": "Boolean",
        "config": {
          "label": "Has Booking",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      },
      "telephone": {
        "type": "Link",
        "config": {
          "label": "Telephone",
          "placeholder": "",
          "select": null,
          "allowTargetBlank": true,
          "allowText": true,
          "repeat": false
        }
      }
    }
  }
}
